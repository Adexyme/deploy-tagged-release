{
  "name": "your-vendor/laravel-deploy-tagged-release",
  "description": "Laravel CLI command to deploy a tagged release with custom schema (optional).",
  "license": "MIT",
  "authors": [
    { "name": "Adegoke Akintoye "email": "adexyme@gmail.com" }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "YourVendor\\DeployRelease\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourVendor\\DeployRelease\\DeployReleaseServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
